ac_unique_file="src/stanley.tcl"
ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT CXX CXXFLAGS ac_ct_CXX gcc_path LN_S SET_MAKE CPP EGREP JAVA_HOME GNU_ENV OSTYPE PS_CMD STANLEY_ROOT TOOLS_ROOT LIVINGSTONE_ROOT SUPPORT_ROOT X11_LIB_DIR X11_INC_DIR CPP_LIB GENERIC_LIBS STANLEY_TCL_TK_VERSION STANLEY_TCL_VERSION STANLEY_TK_VERSION STANLEY_ITCL_VERSION STANLEY_ITCL_LIB_VERSION STANLEY_TKTABLE_VERSION STANLEY_TCL_L STANLEY_TK_L STANLEY_ITCL_L STANLEY_TCL_TK_INC STANLEY_TCL_TK_LIB STANLEY_TKTABLE_LIB STANLEY_TKTABLE_L STANLEY_MICO_VERSION STANLEY_MICO_LIB_VERSION STANLEY_COMBAT_VERSION TCL_MICO_BIN TCL_MICO_LIB TCL_MICO_LIB_L MICO_LIB MICO_LIB_L MICO_LIB_LIR STANLEY_CORBA_P OTHER_MICO_LIBS CORBA_DIR LIBOBJS LTLIBOBJS'
ac_subst_files=''

